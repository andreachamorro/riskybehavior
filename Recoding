###Recoding variables 

##skl_gra
summary(df$skl_gra)
df$grades[df$skl_gra =="Mostly A's"] <- 1
df$grades[df$skl_gra =="Mostly B's"] <- 2
df$grades[df$skl_gra =="Mostly C's"] <- 3
df$grades[df$skl_gra =="Mostly D's"] <- 4
df$grades[df$skl_gra =="Mostly E's or F's"] <- 5
count(df$grades)

##gang
summary(df$gang)
df$ingang[df$gang =="Yes"] <- 1
df$ingang[df$gang =="No"] <- 0
count(df$ingang)
